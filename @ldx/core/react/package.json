  "description": "LDX designsystem",
  "author": "Aksel | NAV designsystem team",
  "license": "UNLICENSED",
  "keywords": [
    "react",
    "typescript",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariusrostad/ldx.no.git"
  },
  "sideEffects": false,
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "typings": "./esm/index.d.ts",
  "files": [
    "/cjs",
    "/esm",
    "/src",
    "_docs.json",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "create-package-json-pointers-to-esm": "node ../../../scripts/createPackageJsonsWithESMPointers.js",
    "clean": "rimraf cjs esm .turbo",
    "build": "concurrently \"tsc\" \"tsc -p tsconfig.esm.json\" && pnpm create-package-json-pointers-to-esm",
    "watch": "tsc --watch -p tsconfig.esm.json",
    "test": "TZ=UTC jest"
  },
  "dependencies": {
    "@floating-ui/react": "0.22.3",
    "clsx": "^1.2.1"
  },
  "devDependencies": {
    "@testing-library/dom": "9.2.0",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.0",
    "@types/jest": "^29.0.0",
    "@types/react-modal": "^3.13.1",
    "concurrently": "^8.0.1",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "rimraf": "3.0.2",
    "swr": "^1.1.2",
    "ts-jest": "^29.0.0",
    "ts-node": "10.9.0",
    "typescript": "^4.8.0"
  },
  "peerDependencies": {
    "@types/react": "^17.0.30 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  }
}
